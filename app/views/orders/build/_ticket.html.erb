<%= tickets.fields_for ticket.id.to_s, ticket do |f| %>
  <tr>
    <td>
      <%= ticket.access_level.name %>
    </td>
    <td>
      <div class="form-group">
        <%= f.text_field :name, class: 'form-control' %>
      </div>
    </td>
    <td>
      <div class="form-group">
        <%= f.email_field :email, class: 'form-control' %>
      </div>
    </td>
  </tr>
  <% if ticket.errors.any? %>
    <tr>
      <td colspan="5">
        <%= form_errors ticket %>
      </td>
    </tr>
  <% end %>
<% end %>
